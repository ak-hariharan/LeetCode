class Solution {
    public List<Integer> findWordsContaining(String[] words, char x) {
        List<Integer> outPutArr= new ArrayList<>();
        for(int i = 0; i < words.length; i += 1){
            for(int j = 0; j < words[i].length(); j += 1){
                if(words[i].charAt(j) == x){
                      outPutArr.add(i);
                    break;
                }
                  
            }
                
        }
        return outPutArr;
    }
}
